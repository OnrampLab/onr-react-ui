{
  "name": "@onr/plugin-custom-admin-dashboard",
  "private": true,
  "version": "0.3.3",
  "description": "custom dashboard plugin",
  "license": "ISC",
  "main": "lib/index.js",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "tsc --watch --preserveWatchOutput",
    "lint": "prettier -c 'src/**/*'; eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*'; eslint src --fix --ext .ts,.tsx",
    "build": "tsc --build tsconfig.json",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "yarn test --watch",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/OnrampLab/onr-react-ui/issues"
  },
  "dependencies": {
    "@onr/core": "^0.15.2",
    "@onr/plugin-antd": "^0.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^4.3.1"
  },
  "peerDependencies": {
    "next": "^13.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@onr/eslint-config": "^0.4.1",
    "@onr/prettier-config": "^1.2.1",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "eslint": "^8.40.0",
    "typescript": "^4.5.2"
  },
  "prettier": "@onr/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
