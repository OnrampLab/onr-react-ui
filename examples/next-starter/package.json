{
  "name": "next-starter",
  "private": true,
  "version": "0.20.1",
  "description": "",
  "main": "src/server/index.js",
  "workspaces": [
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "dev": "lerna run dev --parallel",
    "dev:old": "lerna run dev:old",
    "build": "lerna run build --stream",
    "start:old": "lerna run start:old",
    "start": "lerna run start --parallel",
    "heroku-prebuild": "npm i --dev && NODE_ENV=production next build",
    "export": "lerna run export",
    "analyze": "lerna run analyze",
    "cli": "node ./project-cli/dist/index.js a",
    "_postinstall": "husky install",
    "build-project-cli": "cd project-cli && npm i && npm run build",
    "test": "lerna run test",
    "test:ci": "lerna run test:ci",
    "lint": "lerna run lint",
    "format": "lerna run format",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "next-starter-app": "^0.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^5.0.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.(js|ts|tsx)": [
      "eslint -c .eslintrc.js --fix",
      "prettier --write"
    ]
  }
}
