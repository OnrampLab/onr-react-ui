{
  "name": "next-starter",
  "private": true,
  "version": "0.21.0",
  "description": "",
  "main": "src/server/index.js",
  "workspaces": [
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:old": "turbo run dev:old",
    "build": "turbo run build",
    "start:old": "turbo run start:old",
    "start": "turbo run start --parallel",
    "heroku-prebuild": "npm i --dev && NODE_ENV=production next build",
    "export": "turbo run export",
    "analyze": "turbo run analyze",
    "cli": "node ./project-cli/dist/index.js a",
    "_postinstall": "husky install",
    "build-project-cli": "cd project-cli && npm i && npm run build",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "next-starter-app": "^0.4.5",
    "turbo": "^1.9.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@onr/commitlint-config": "^1.1.0",
    "husky": "^5.0.0",
    "lint-staged": "^10.2.11"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.(js|ts|tsx)": [
      "eslint -c .eslintrc.js --fix",
      "prettier --write"
    ]
  },
  "packageManager": "^yarn@1.22.19"
}
