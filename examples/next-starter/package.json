{
  "name": "next-starter",
  "private": true,
  "version": "0.37.0",
  "description": "",
  "main": "src/index.js",
  "workspaces": [
    "apps/*",
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "start": "turbo run start --parallel",
    "heroku-prebuild": "npm i --dev && NODE_ENV=production next build",
    "export": "turbo run export",
    "analyze": "turbo run analyze",
    "cli": "node ./project-cli/dist/index.js a",
    "_postinstall": "husky install",
    "build-project-cli": "cd project-cli && npm i && npm run build",
    "test": "turbo run test",
    "test:ci": "turbo run test --concurrency=1",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@onr/commitlint-config": "^1.4.3",
    "@onr/eslint-config": "^0.6.6",
    "@onr/prettier-config": "^2.0.1",
    "@onr/tsconfig": "^1.0.1",
    "prettier": "^2.8.8",
    "syncpack": "^11.2.1",
    "tslib": "^2.6.3",
    "tsup": "^8.2.4",
    "turbo": "^2.0.14"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "author": "",
  "license": "ISC",
  "prettier": "@onr/prettier-config",
  "lint-staged": {
    "src/**/*.(js|ts|tsx)": [
      "eslint -c .eslintrc.js --fix",
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.4.0"
}
