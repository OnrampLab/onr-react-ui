{
  "name": "@onr/plugin-custom-antd-starter",
  "private": true,
  "version": "0.1.2",
  "description": "custom antd starter plugin",
  "license": "ISC",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "concurrently npm:bundle:watch npm:type:gen:watch",
    "lint": "prettier -c 'src/**/*'; eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*'; eslint src --fix --ext .ts,.tsx",
    "build": "yarn type:gen ; yarn bundle",
    "bundle": "tsup",
    "bundle:watch": "tsup --watch",
    "type:gen": "tsc --emitDeclarationOnly",
    "type:gen:watch": "tsc --emitDeclarationOnly --watch --preserveWatchOutput",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "yarn test --watch",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/OnrampLab/onr-react-ui/issues"
  },
  "dependencies": {
    "@onr/core": "^0.42.0",
    "@onr/plugin-antd": "^0.29.2",
    "antd": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.7"
  },
  "peerDependencies": {
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@onr/eslint-config": "^0.6.5",
    "@onr/prettier-config": "^2.0.0",
    "@types/node": "^14.14.21",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "prettier": "@onr/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
