{
  "name": "@onr/plugin-antd",
  "version": "1.0.1",
  "description": "core-antd package",
  "homepage": "https://github.com/OnrampLab/onr-react-ui#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OnrampLab/onr-react-ui.git"
  },
  "scripts": {
    "dev": "concurrently npm:bundle:watch npm:type:gen:watch",
    "lint": "prettier -c 'src/**/*'; eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*'; eslint src --fix --ext .ts,.tsx",
    "build": "yarn type:gen ; yarn bundle",
    "bundle": "tsup",
    "bundle:watch": "tsup --watch",
    "type:gen": "tsc --emitDeclarationOnly",
    "type:gen:watch": "tsc --emitDeclarationOnly --watch --preserveWatchOutput",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "yarn test --watch",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/OnrampLab/onr-react-ui/issues"
  },
  "dependencies": {
    "http-status": "^1.5.0",
    "radash": "^12.1.0"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.4.0",
    "@onr/core": "^1.0.1",
    "antd": "^5.20.3",
    "next": "^13.5.6",
    "next-auth": "^4.24.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^7.2.2",
    "redux": "^4.2.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^6.1.2",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.4.0",
    "@onr/core": "^1.0.1",
    "@onr/eslint-config": "^0.6.6",
    "@onr/prettier-config": "^2.0.1",
    "@onr/tsconfig": "^1.0.1",
    "@types/node": "^14.14.21",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "antd": "^5.20.3",
    "change-case": "^4.1.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "next": "^13.5.6",
    "next-auth": "^4.24.5",
    "prettier": "^2.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^7.2.2",
    "redux": "^4.2.1",
    "redux-thunk": "^2.3.0",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "prettier": "@onr/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
