{
  "name": "@onr/core",
  "version": "1.0.0",
  "description": "core module",
  "author": "Onramp Lab DEV <dev@onramplab.com>",
  "homepage": "https://github.com/OnrampLab/onr-react-ui#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.js"
    }
  },
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OnrampLab/onr-react-ui.git"
  },
  "scripts": {
    "dev": "concurrently npm:bundle:watch npm:type:gen:watch",
    "lint": "prettier -c 'src/**/*'; eslint src --ext .ts,.tsx",
    "format": "prettier --write 'src/**/*'; eslint src --fix --ext .ts,.tsx",
    "build": "yarn type:gen ; yarn bundle",
    "bundle": "tsup",
    "bundle:watch": "tsup --watch",
    "type:gen": "tsc --emitDeclarationOnly",
    "type:gen:watch": "tsc --emitDeclarationOnly --watch --preserveWatchOutput",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "yarn test --watch",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/OnrampLab/onr-react-ui/issues"
  },
  "dependencies": {
    "@onr/common": "^1.0.0",
    "minimatch": "^3.0.4",
    "swr": "^0.5.6"
  },
  "peerDependencies": {
    "@onr/logging": "^1.0.0",
    "@onr/ts-rest-client": "^3.0.0",
    "antd": "^5.10.0",
    "lodash": "^4.17.21",
    "next": "^13.5.6",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.2",
    "redux": "^4.2.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@onr/eslint-config": "^0.6.5",
    "@onr/logging": "^1.0.0",
    "@onr/prettier-config": "^2.0.0",
    "@onr/ts-rest-client": "^3.0.0",
    "@types/lodash": "^4.17.7",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^14.14.21",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.12",
    "antd": "^5.10.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "lodash": "^4.17.21",
    "next": "^13.5.6",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-redux": "^7.2.2",
    "redux": "^4.2.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^6.0.7",
    "tslib": "^2.6.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "prettier": "@onr/prettier-config",
  "publishConfig": {
    "access": "public"
  }
}
